{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2018",
    "lib": ["esnext", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "experimentalDecorators": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "keyofStringsOnly": true,
    "noImplicitAny": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "paths": {}
  }
}
